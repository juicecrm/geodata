{
    "name": "juicecrm/i18n-data",
    "description": "Internationalization Data for Laravel 10 and later",
    "type": "library",
    "license": "GLPv3",
    "autoload": {
        "psr-4": {
            "JuiceCrm\\I18nData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuiceCrm\\I18nData\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "JuiceCRM",
            "email": "info@juicecrm.works"
        }
    ],
	"extra": {
		"laravel": {
			"providers": [
				"JuiceCRM\\I18nData\\I18nDataServiceProvider"
			],
			"aliases": {
				"Country": "JuiceCRM\\I18nData\\Facades\\Country"
			}
		}
	},
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/database": "^10.0"
    }
}
