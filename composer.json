{
    "name": "juicecrm/geodata",
    "description": "Geographical Data for Laravel 10 and later",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "JuiceCRM\\GeoData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuiceCRM\\GeoData\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "JuiceCRM",
            "email": "info@juicecrm.works"
        }
    ],
	"extra": {
		"laravel": {
			"providers": [
				"JuiceCRM\\GeoData\\GeoDataServiceProvider"
			],
			"aliases": {
				"Country": "JuiceCRM\\GeoData\\Facades\\Country"
			}
		}
	},
	"require": {
		"php": "^8.1||^8.2",
        "ext-zip": "*"
	},
    "require-dev": {
        "illuminate/console": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.6",
        "phpoption/phpoption": "^1.9",
        "vlucas/phpdotenv": "^5.5",
        "orchestra/testbench": "^8.5",
        "guzzlehttp/guzzle": "^7.5"
    }
}
